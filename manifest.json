{
	"manifest_version": 2,
	"name": "productivity",
	"version": "0.1",
	"background": {
		"scripts": [ "./popup/runTimer.js", "./popup/stopTimer.js" ]
	},

	"browser_action": {
		"default_icon": "tab-icon.png",
		"default_title": "Get back to work!",
		"default_popup": "./popup/popup.html"
	},
	"chrome_url_overrides": { "newtab": "./newTab/newTab.html" },
	"permissions": [ "activeTab", "storage" ]
}
